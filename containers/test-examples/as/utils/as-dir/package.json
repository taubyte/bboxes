{
  "name": "starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "asc assembly/index.ts -o build/artifact.wasm --config node_modules/@assemblyscript/wasi-shim/asconfig.json --runtime minimal",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:release"
  },
  "dependencies": {
    "sdk": "https://bitbucket.org/taubyte/as-sdk",
    "@assemblyscript/wasi-shim": "^0.1.0",
    "assemblyscript": "^0.25.1",
    "typescript": "^4.9.4"
  },

  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}
